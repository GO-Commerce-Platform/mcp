# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASS=postgres
DB_NAME=gocommerce

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# Server Configuration
PORT=3000
NODE_ENV=development

# GO-Commerce Integration
GOCOMMERCE_SERVER_URL=http://localhost:8080
GOCOMMERCE_API_KEY=your-api-key-here

# Security
JWT_SECRET=your-jwt-secret-here
ACCESS_TOKEN_EXPIRY=1h
REFRESH_TOKEN_EXPIRY=7d

# Logging
LOG_LEVEL=debug
LOG_FORMAT=json

# Feature Flags
ENABLE_CODE_GENERATION=true
ENABLE_MODEL_SUGGESTIONS=true
ENABLE_SCHEMA_ANALYSIS=true

# Cache Settings
CACHE_TTL=3600
SCHEMA_CACHE_TTL=86400

# Rate Limiting
RATE_LIMIT_WINDOW=15m
RATE_LIMIT_MAX=100

# Development
DEBUG=true
PRETTY_PRINT=true
